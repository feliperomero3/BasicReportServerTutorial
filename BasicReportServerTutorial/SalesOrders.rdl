  <DataSources>
    <DataSource Name="AdventureWorks2012">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=ITJROMERO\DEVELOPER;Initial Catalog=AdventureWorks2012</ConnectString>
        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>f8a6d879-870e-4e54-8908-855c673c8cfc</rd:DataSourceID>
